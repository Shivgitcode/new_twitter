{
  "version": 2,
  "buildCommand": "pnpm run vercel-build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "src/bin.ts",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist",
        "includeFiles": ["node_modules/.bin/tsc"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
