{
    "version": 2,
    "buildCommand": "pnpm run build",
    "devCommand": "pnpm run dev",
    "installCommand": "pnpm install",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
}